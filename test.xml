[formats]
# 日志格式
# %F 绝对路径 %f只保留文件名
# 带线程id日志输出格式
format_with_th_id = "[%-5V] [%f:%L][thr_id:%t] %m%n"

# 不带线程id日志输出格式
format_no_th_id = "[%-5V] [%f:%L] %m%n"

[rules]
# 主程序日志配置
log_all.*		>stdout; format_with_th_id
log_all.*		"../log/log.txt"; format_with_th_id

# 收数据测试程序日志输出配置
log_recv_test.*		>stdout; format_no_th_id
log_recv_test.*		"../log/log_recv_text.txt"; format_no_th_id

# 发数据测试程序日志输出配置
log_send_test.*		>stdout; format_no_th_id
log_send_test.*		"../log/log_send_test.txt"; format_no_th_id
# log_all.*		"../log/%t.txt"
# log_all.=error   "error.log"
# log_all.=notice  "rw.c.log"



; 数据来源的ip还有端口
[recv_test]
ip = 0.0.0.0
port = 8080


; 数据来源的ip还有端口
[send_test]
; ip = 10.42.0.162
ip = 192.168.199.104
port = 8081

; 传输文件的路径，可以绝对路径，也可以相对路径（相对路径工作目录为可执行文件的目录）
[file_to_send]
path = ../test.xml